{
  "name": "klaxoon_bookmark",
  "version": "0.0.1-dev",
  "description": "Bookmark Media from Vimeo or Flickr",
  "scripts": {
    "client:start": "cd client && webpack-dev-server --mode development --open --hot",
    "client:build": "cd client && webpack --config webpack.prod.js --mode production",
    "client:lint": "eslint './client/src/**/*.{js,jsx}' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --config-path .prettierrc.js --write",
      "eslint --config .eslintrc.js --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienlocatelli/klaxoonBookmark.git"
  },
  "author": "julien.locatelli@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julienlocatelli/klaxoonBookmark/issues"
  },
  "homepage": "https://github.com/julienlocatelli/klaxoonBookmark#readme",
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-transform-async-to-generator": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.8.3",
    "@commitlint/cli": "12.0.0",
    "@commitlint/config-conventional": "12.0.0",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "5.0.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.3.8",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "@rematch/core": "2.0.1",
    "i18next": "19.9.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-datepicker": "3.6.0",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.4",
    "react-hot-loader": "4.13.0",
    "react-i18next": "11.8.8",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "styled-components": "5.2.1",
    "styled-system": "5.1.5"
  }
}
